{
  "compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"allowUnreachableCode": true,
    "allowJs": false,
    "declaration": false,
    "lib": [ "dom", "es5", "es2015", "es2015.promise" ],
    "module": "commonjs",
    "noImplicitAny": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  }
}